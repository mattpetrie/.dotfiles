[user]
	name = Matt Petrie
	email = matthew.petrie@gmail.com
[alias]
	aa = add --all
	br = branch
	ci = commit
	co = checkout
	df = diff
	last  = log -1 HEAD
	lg = log -p
	st = status
	unstage = reset HEAD --
[core]
	editor = code --wait
	excludesfile = ~/.gitignore
[push]
	default = simple
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
